<%-include('partials/header')%>
  <title>Generate API Key</title>
  </head>
  <body>
    <%-include('partials/navbar')%>
    <h1 class="heading">Generate API Key</h1>
    <form action="/api/generate" method="post">
      <label for="generateApiKey">Do you want to generate an API key?</label>
      <input
        type="checkbox"
        name="generateApiKey"
        id="generateApiKey"
        value="yes"
        required
      />
      <br />
      <button type="submit">Generate API Key</button>
    </form>
    <%-include('partials/last')%>
